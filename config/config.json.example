{
  "mcpProxy": {
    "name": "MCP Proxy",
    "version": "1.0.0",
    "type": "stdio",
    "hierarchyPath": "~/.claude/mcp-proxy/hierarchy",
    "options": {
      "lazyLoad": true,
      "preloadAll": true
    }
  },
  "mcpServers": {
    "example-server": {
      "transportType": "stdio",
      "command": "python",
      "args": ["/path/to/your/mcp_server.py"],
      "env": {
        "API_KEY": "your-api-key-here"
      },
      "options": {
        "lazyLoad": true
      }
    },
    "another-server": {
      "transportType": "stdio",
      "command": "node",
      "args": ["/path/to/your/mcp_server.js"],
      "env": {},
      "options": {
        "lazyLoad": true
      }
    }
  }
}
