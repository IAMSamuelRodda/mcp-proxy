{
  "tools": {
    "mailjet_list_messages": {
      "description": "List messages with pagination and date filtering.\n\nRetrieve sent messages with optional date range filtering.\nSupports pagination with limit and offset parameters.",
      "maps_to": "mailjet_list_messages",
      "server": "mailjet",
      "inputSchema": {
        "type": "object",
        "properties": {
          "limit": {
            "type": "integer",
            "description": "Maximum messages to return (default 50)"
          },
          "offset": {
            "type": "integer",
            "description": "Pagination offset"
          },
          "from_date": {
            "type": "string",
            "description": "Start date filter (YYYY-MM-DD)"
          },
          "to_date": {
            "type": "string",
            "description": "End date filter (YYYY-MM-DD)"
          },
          "response_format": {
            "type": "string",
            "enum": ["markdown", "json"],
            "description": "Output format: 'markdown' or 'json'"
          }
        },
        "required": [],
        "additionalProperties": false
      }
    }
  }
}
